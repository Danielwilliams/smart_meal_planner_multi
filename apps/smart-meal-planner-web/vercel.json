{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "^/api/instacart/(.*)",
      "dest": "https://smartmealplannermulti-development.up.railway.app/instacart/$1"
    },
    {
      "src": "^/api/(.*)",
      "dest": "https://smartmealplannermulti-development.up.railway.app/$1"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/(.*)$",
      "dest": "/index.html"
    }
  ]
}